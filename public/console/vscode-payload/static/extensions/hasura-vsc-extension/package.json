{
  "name": "hasura",
  "displayName": "Hasura",
  "description": "VSC Web Extension for accessing files on the hasura instances",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:hasura-sidebar",
    "onCommand:hasura.connectCommand",
    "onCommand:hasura.disconnectCommand",
    "onCommand:hasura.getGraphQLSchema",
    "onCommand:hasura.exportMetadataCommand",
    "onCommand:hasura.importMetadataCommand",
    "onCommand:hasura.debugCommand"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hasura.connectCommand",
        "title": "Hasura: Connect"
      },
      {
        "command": "hasura.disconnectCommand",
        "title": "Hasura: Disconnect"
      },
      {
        "command": "hasura.getGraphQLSchema",
        "title": "Hasura: Fetch GraphQL Schema"
      },
      {
        "command": "hasura.getCustomActions",
        "title": "Hasura: List all custom actions"
      },
      {
        "command": "hasura.debugCommand",
        "title": "Hasura: Debug"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hasura-sidebar-view",
          "title": "Hasura",
          "icon": "media/hasura-logo.svg"
        }
      ]
    },
    "views": {
      "hasura-sidebar-view": [
        {
          "type": "webview",
          "id": "hasura-sidebar",
          "name": "Hasura",
          "icon": "media/hasura-logo.svg",
          "contextualTitle": "Hasura"
        }
      ]
    },
    "configuration": {}
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile": "webpack --mode production",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@tsconfig/svelte": "^3.0.0",
    "@types/mocha": "^9.1.1",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.71.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-web": "^0.0.29",
    "assert": "^2.0.0",
    "eslint": "^8.20.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "path-browserify": "^1.0.1",
    "request-light": "^0.5.8",
    "uuid": "^9.0.0"
  }
}